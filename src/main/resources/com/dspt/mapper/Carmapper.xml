<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.dspt.mapper.Carmapper">
    <select id="findcar" parameterType="string" resultType="Car">
        select * from car where username=#{username}
    </select>
    <select id="findoen" parameterType="string" resultType="Car">
        select * from car where cid=#{cid}
    </select>
    <select id="findpro" parameterType="string" resultType="Car">
        select * from car where id=#{id} and username=#{username}
    </select>
    <update id="update" parameterType="Car">
        update car set num=#{num} where cid=#{cid}
    </update>
    <insert id="addcar" parameterType="Car">
        insert into car (id,username,num) values (#{id},#{username},#{num})
    </insert>
    <delete id="delcar" parameterType="string">
        delete from car where cid=#{cid}
    </delete>

    <delete id="delall" parameterType="string">
        delete  from car where username=#{username}
    </delete>
</mapper>